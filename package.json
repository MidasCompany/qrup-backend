{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "yarn migrate && yarn db_init && node ./src/server.js",
    "reset": "rm test.sqlite && yarn migrate && yarn seed",
    "migrate": "sequelize db:migrate",
    "migrate_undo": "sequelize db:migrate:undo:all",
    "seed": "sequelize db:seed:all",
    "seed_undo": "sequelize db:seed:undo:all",
    "dev": "yarn migrate && yarn seed  && nodemon --exec node -r dotenv/config src/server.js --ignore test",
    "dev_reset": "yarn seed_undo && yarn migrate_undo && yarn dev",
    "lint": "eslint src/**/*.js",
    "pretest": "cross-env-shell NODE_ENV=test \"rimraf test.sqlite && yarn migrate && yarn seed\"",
    "test": "jest"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cnpj": "^3.0.0",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "factory-girl": "^5.0.4",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.2",
    "mongoose": "^5.8.13",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "rimraf": "^3.0.2",
    "sequelize": "^5.21.2",
    "sequelize-cli": "^5.5.1",
    "sqlite3": "^4.1.1",
    "uuid": "^7.0.3",
    "validar-cpf": "^2.1.2",
    "yup": "^0.28.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "eslint": "^7.1.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "faker": "^4.1.0",
    "jest": "^26.0.1",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2"
  }
}
