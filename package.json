{
	"name": "backend",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"start": "yarn migrate && node ./src/server.js",
		"migrate": "sequelize db:migrate",
		"migrate_undo": "sequelize db:migrate:undo:all",
		"db_init": "yarn db_init_undo && sequelize db:seed:all",
		"db_init_undo": "sequelize db:seed:undo:all",
		"dev": "yarn migrate && nodemon --exec node -r dotenv/config src/server.js --ignore test",
		"dev_reset": "yarn migrate_undo && yarn dev",
		"lint": "eslint src/**/*.js",
		"test": "set NODE_ENV=test && jest"
	},
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"cnpj": "^3.0.0",
		"crypto": "^1.0.1",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"jsonwebtoken": "^8.5.1",
		"mongoose": "^5.8.13",
		"morgan": "^1.9.1",
		"multer": "^1.4.2",
		"pg": "^7.12.1",
		"pg-hstore": "^2.3.3",
		"sequelize": "^5.21.2",
		"sequelize-cli": "^5.5.1",
		"sqlite3": "^4.1.1",
		"uuid": "^7.0.3",
		"validar-cpf": "^2.1.2",
		"yup": "^0.28.0"
	},
	"devDependencies": {
		"jest": "^25.1.0",
		"nodemon": "^2.0.2"
	}
}
